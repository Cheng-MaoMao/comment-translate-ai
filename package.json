{
  "name": "ai-powered-comment-translate-extension",
  "displayName": "Comment Translate for AI",
  "description": "This is an extension of the Comment Translate plugin, enabling it to perform translations by invoking a large model compatible with the OpenAI API.",
  "version": "0.0.3",
  "publisher": "Cheng-MaoMao",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "AI",
    "Chat"
  ],
  "keywords": [
    "translateSource",
    "comment translate",
    "ai",
    "AI translate",
    "翻译",
    "注释",
    "翻訳"
  ],
  "homepage": "https://github.com/Cheng-MaoMao/comment-translate-ai#readme",
  "bugs": {
    "url": "https://github.com/Cheng-MaoMao/comment-translate-ai/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cheng-MaoMao/comment-translate-ai.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "image/logo.png",
  "extensionDependencies": [
    "intellsmi.comment-translate"
  ],
  "contributes": {
    "configuration": {
      "title": "AI Translate Configuration",
      "properties": {
        "aiTranslate.largeModelApi": {
          "type": "string",
          "default": "",
          "markdownDescription": "大模型 API 地址"
        },
        "aiTranslate.largeModelKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "大模型 API Key"
        },
        "aiTranslate.largeModelName": {
          "type": "string",
          "default": "",
          "markdownDescription": "大模型名称"
        },
        "aiTranslate.largeModelMaxTokens": {
          "type": "number",
          "default": 2048,
          "minimum": 0,
          "markdownDescription": "大模型最大 token 数量 (设置为 0 则不限制)"
        },
        "aiTranslate.largeModelTemperature": {
          "type": "number",
          "default": 0.2,
          "minimum": 0,
          "maximum": 2,
          "markdownDescription": "大模型温度参数 (较低的值产生更确定的结果，较高的值产生更多样的结果)"
        }
      }
    },
    "translates": [
      {
        "translate": "ai-powered-comment-translate-extension",
        "title": "AI translate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^1.x.x",
    "comment-translate-manager": "^0.0.2",
    "querystring": "^0.2.1"
  }
}